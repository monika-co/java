public class Main {
    public static void main(String[] args) {
        Scanner console = new Scanner(System.in);
        System.out.println("Enter number: ");
       int num = console.nextInt();

       for(int i = 1; i <=10; i++) {
           System.out.printf("%d * %d = %d%n", num, i, num*i);
       }


    }
}
